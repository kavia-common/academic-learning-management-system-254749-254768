# Frontend required env vars
REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_ANON_KEY=
# Used for Supabase emailRedirectTo and CORS/display URLs
REACT_APP_FRONTEND_URL=http://localhost:3000

# Optional integration variables
# If provided and the corresponding feature flag enables backend usage,
# privileged actions will be routed to the backend with Supabase auth token attached.
REACT_APP_BACKEND_URL=http://localhost:8000

# Feature flags are a JSON string. Example:
# {"use_backend": true, "use_direct_supabase": true}
# - use_backend: when true and REACT_APP_BACKEND_URL is set, api.js will call backend for privileged operations
# - use_direct_supabase: when true, keep direct Supabase CRUD for courses (phase 1)
REACT_APP_FEATURE_FLAGS={"use_backend": false, "use_direct_supabase": true}

# Optional additional config supported by container
REACT_APP_API_BASE=
REACT_APP_WS_URL=
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/api/v1/health
REACT_APP_EXPERIMENTS_ENABLED=false
